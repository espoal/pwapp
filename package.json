{
  "name": "pwa",
  "scripts": {
    "serve": "firebase emulators:start",
    "build": "node build.mjs prod",
    "watch": "node build.mjs dev",
    "ssr": "node dist/public/ssr.mjs",
    "critical": "node libs/critical/index.mjs"
  },
  "workspaces": [
    "dist",
    "docs",
    "ext/*",
    "libs/components/*",
    "libs/layouts/*",
    "libs/*",
    "main",
    "pkgs/*",
    "vendors/*"
  ],
  "packageManager": "yarn@4.0.0-rc.14",
  "dependencies": {
    "@espoal/build": "workspace:^"
  }
}
